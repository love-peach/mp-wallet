/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
}

.app-page-content {
  padding-right: 30rpx;
  padding-left: 30rpx;
}

.app-page-has-tel {
  margin-bottom: 80rpx;
}

.app-title-bar {
  position: relative;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  padding-left: 20rpx;
  margin-bottom: 20rpx;
  font-size: 30rpx;
  color: #333;
  font-weight: 600;
  line-height: 40rpx;
}
.app-title-bar:before {
  content: ' ';
  position: absolute;
  left: 0;
  top: 28rpx;
  width: 4rpx;
  height: 28rpx;
  background: #4ca6ff;
}

@import "/common/template.wxss";
@import "/common/certification-warning-text/index.wxss";
@import "/common/certification-warning-text-confirm/index.wxss";
@import "/common/footer-tel/index.wxss";
