.message-box-wrap {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 360rpx;
}
.message-box-icon {
  width: 128rpx;
  height: 128rpx;
}
.message-box-text {
  margin-top: 15rpx;
  font-size: 36rpx;
}
.message-box-text-warning {
  color: #ff9600;
}
.message-box-text-negative{
  color: #c5c5c5
}
.message-box-text-success {
  color: #40db89;
}
